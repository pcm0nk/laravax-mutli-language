    "@nuxt/devtools": "^1.3.9",
    "@nuxtjs/i18n": "^8.5.1",
    "@types/js-cookie": "^3.0.6",
    "@vueuse/nuxt": "^10.11.0",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
